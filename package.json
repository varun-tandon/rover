{
  "name": "rover",
  "version": "1.0.0",
  "description": "AI-powered codebase scanner with multi-agent validation",
  "type": "module",
  "bin": {
    "rover": "./dist/cli.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "dev": "tsx src/cli.tsx",
    "build": "tsc && cp -r src/fix/prompts dist/fix/",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "codebase",
    "scanner",
    "claude",
    "lint"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "ignore": "^7.0.5",
    "ink": "^6.5.1",
    "ink-spinner": "^5.0.0",
    "meow": "^14.0.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
